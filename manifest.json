{
    "manifest_version": 2,
    "name": "Goodlink",
    "version": "1.0.0",
    "description": "Auto-generated links from Amazon book pages for Goodreads",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ]
}
